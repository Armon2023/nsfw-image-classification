.progress_pdca{position:relative;}
.progress_pdca .progress_green_notstarted0{width: 100%;height: 4px;background: #c8ccd0;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_green_initialized1{width: 100%;height: 4px;background: #90ad57;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_blue_inprogress2{width: 100%;height: 4px;background: #3aab6d;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_blue_submited6{width: 100%;height: 4px;background: #3d85b5;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_completed9{width: 100%;height: 4px;background: #86699a;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_blocking20{width: 100%;height: 4px;background: #0ba0b9;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_red_delay30{width: 100%;height: 5px;background: #ff5b4a;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_black_hang40{width: 100%;height: 6px;background: #000000;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}
.progress_pdca .progress_yellow_rejected50{width: 100%;height: 4px;background: #af2727;position:absolute;  bottom:0px;  left:0px; right: 0px; cursor:pointer}

.progress_pdca .progress_pdca ul{list-style: none; }
.progress_pdca ul li{display:inline-block; margin-right: 6px;float:left}
.progress_pdca ul li.last{float: right; margin-right: 0px}
.progress_pdca ul li span.remind{float: right;height:16px; width: 16px; background-image: url(../images/icons/control/16/full-time.png); margin:1px 0px 0px 2px;}

li.statusBarBlock{border:none;float:right;}
li.statusBarBlock ul{display:inline-block;width:90px}
.controlC ul li.statusBar{padding:0 5px;border:none;background:none;box-shadow:none;-webkit-box-shadow:none;float:right;display:block;margin-top:5px;}
.controlC ul li.statusBar div{width:85px;height:50px;float:left;}
.controlC ul li.statusBar p{height:20px;float:left;margin-top:5px;}
li.statusBar span{float:left;text-align:right;}
li.statusBar i{width:30px;height:12px;font-size:12px;color:#fff;display:inline-block;margin-top:3px; margin-left:2px}
li.statusBar i.notstarted{background:#fdc19f;border:1px solid #303030;}
li.statusBar i.initialized{background:#90ad57;border:1px solid #303030;}
li.statusBar i.inprogress{background:#3aab6d;border:1px solid #303030;}
li.statusBar i.completed{background:#86699a;border:1px solid #303030;}
li.statusBar i.submitted{background:#3d85b5;border:1px solid #303030;}
li.statusBar i.rejected{background:#af2727;border:1px solid #303030;}
li.statusBar i.blocking{background:#0ba0b9;border:1px solid #303030;}
li.statusBar i.delay{background:#ff5b4a;border:1px solid #303030;}
li.statusBar i.hang{background:#000000;border:1px solid #303030;}

.progress_pdca ul li.notstarted{border-bottom:3px solid #fdc19f;}
.progress_pdca ul li.inprogress{border-bottom:3px solid #3aab6d;}
.progress_pdca ul li.submitted{border-bottom:3px solid #3d85b5; background:#c9eaff}
.progress_pdca ul li.rejected{border-bottom:3px solid #af2727;background:#ffdd82}
.progress_pdca ul li.completed{border-bottom:3px solid #86699a;}


