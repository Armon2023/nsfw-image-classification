.form input[type=text]:focus, .form input[type=password]:focus, .form textarea:focus { background: #fff; -webkit-box-shadow: 0 0 0 2px #f4f4f4; box-shadow: 0 0 0 2px #f4f4f4; -moz-box-shadow: 0 0 0 2px #f4f4f4; }
.form input[disabled]{ background: #eee; /*border: 1px solid #e8e8e8;*/ color: #303030; }
