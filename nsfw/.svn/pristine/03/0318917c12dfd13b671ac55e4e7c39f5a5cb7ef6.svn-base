.formError .formErrorArrow .line1 { width: 1px; border: none; background: #ddd; }

.checker input, .radio input, .selector select { cursor: pointer; }


/* ===== Select ===== */

div.selector { position: relative; min-height: 28px; background: url(../images/forms/select_left2.png) no-repeat top left; width: 200px; position: relative;  }
div.selector select {width: 200px; font-size: 12px; position: absolute; height: 28px; top: 0; left: 0; }
