}
.notification.yellow {
  background-color: #ffcc66;
  color: #ffffff;
}
.notification.orange {
  background-color: #f4b162;
}
.notification.purple {
  background-color: #af91e1;
}
.notification.pink {
  background-color: #f78db8;
}
