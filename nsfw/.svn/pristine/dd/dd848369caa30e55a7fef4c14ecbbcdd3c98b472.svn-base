            margin-top: 0px;
        }

        .systemtitle {
            float: right;
            color: #fff;
            margin: 20px 134px 0px 20px;
        }

        .systemtitle p img {
            width: 500px;
        }

        .loginPage {
            display: none;
            position: fixed;
            left: 25%;
            top: 150px;
            z-index: 99;
            width: 50%;
        }

        .close:hover {
            cursor: pointer;
        }
